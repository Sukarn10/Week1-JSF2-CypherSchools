//Dynamic Polymorphism: overriding

abstract class Doctor {
    abstract void treatPatient();
}
class Physician extends Doctor{

    @Override
    void treatPatient() {

        System.out.println("Hi! I am Physician \nI Treat Patients using Medicines.");

    }

}

class Surgeon extends Doctor{ I void treatPatient() {

    @Override

            System.out.println("Hi! I am Surgeon \nI Treat Patients using Surgery!");

}

}

@Override

class Dentist extends Doctor{ System.out.println("Hi! I am Dentist \nI Treat Patients Teeth");

    void treatPatient() {

    }

    public class Program {

    }
